<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Duvido! @ViewBag.Title</title>
    <link rel="shortcut icon" type="imagex/png" href="~/Imagens/Universal/Imagens/logo menor.ico"/>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,500;1,100&display=swap" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <audio autoplay id="music">
        <source src="~/Audio/Anonimo/musicafundo.mp3" type="audio/ogg">

    </audio>
    <div class="container main shadow-lg">
    
        <div class="row">
            <div class="col">
            </div>
            <div class="col">
                <div id="logo"></div>
            </div>
            <div class="col">

                <div class="audio-controls" id="mute" style="margin-left: auto;">
                    <button id="muteButton">Mute/Unmute</button>
                </div>


            </div>


        </div>
    @RenderBody()
    </div>
      
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
